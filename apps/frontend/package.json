{
  "private": true,
  "name": "@flightcore/frontend",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "start": "astro start",
    "check": "astro check",
    "dev": "dotenvx run -f .env.development -- astro dev",
    "build": "dotenvx run --verbose -- astro build",
    "build:production": "dotenvx run -f .env.production --verbose -- astro build",
    "build:prerelease": "dotenvx run -f .env.prerelease --verbose -- astro build",
    "build:development": "dotenvx run -f .env.development --verbose -- astro build",
    "preview": "node dist/server/entry.mjs",
    "optimize:fonts": "subfont -ir",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "make:env": "NODE_ENV=${NODE_ENV:-development} sh make-dot-env.sh",
    "inject:env": "dotenvx run",
    "lint": "eslint",
    "postinstall": "pnpm make:env"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^3.1.9",
    "@astrojs/node": "^8.3.4",
    "@astrojs/react": "^3.6.2",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/tailwind": "^5.1.2",
    "@flightcore/booking": "workspace:*",
    "@flightcore/uikit": "workspace:*",
    "@nanostores/react": "^0.7.3",
    "@types/aos": "^3.0.7",
    "aos": "3.0.0-beta.6",
    "astro": "^4.16.13",
    "astro-compressor": "^0.4.1",
    "astro-font": "^0.1.81",
    "astro-icon": "^1.1.2",
    "axios": "^1.7.7",
    "blurhash": "^2.0.5",
    "clsx": "^2.1.1",
    "embla-carousel": "^8.4.0",
    "embla-carousel-auto-scroll": "^8.4.0",
    "embla-carousel-autoplay": "^8.4.0",
    "embla-carousel-class-names": "^8.4.0",
    "embla-carousel-fade": "^8.4.0",
    "embla-carousel-react": "^8.4.0",
    "express": "^4.21.1",
    "nanostores": "^0.11.3",
    "photoswipe": "^5.4.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "subfont": "^7.2.1",
    "typescript": "5.5.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@types/eslint__js": "^8.42.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/parser": "^8.13.0",
    "astro-nanointl": "^1.0.0",
    "eslint": "9.14.0",
    "eslint-mdx": "^3.1.5",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-mdx": "^3.1.5",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^15.12.0",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "svgo": "^3.3.2",
    "typescript": "5.5.4",
    "typescript-eslint": "^8.13.0"
  }
}
